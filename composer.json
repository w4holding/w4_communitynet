{
    "name": "w4services/w4_communitynet",
    "type": "typo3-cms-extension",
    "version": "1.0.1",
    "description": "W4 Basic Extension for CommunityNet websites.",
    "homepage": "https://www.w-4.com",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/w4holding/w4_communitynet.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/w4holding/w4_organization_list.git"
        }
    ],
    "authors": [{
        "name": "W4 Services GmbH",
        "email": "info@w-4.com",
        "role": "Developer"
    }],
    "license": [
        "GPL-2.0-or-later"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "typo3/cms-core": "^11.5",
        "b13/container": "^1.6",
        "brotkrueml/schema": "^2.5",
        "fluidtypo3/vhs": "^6.1",
        "friendsoftypo3/tt-address": "^6.1",
        "georgringer/eventnews": "^5.0",
        "georgringer/news": "^9.4",
        "lochmueller/faq": "^5.1",
        "w4services/w4_organization_list": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "W4Services\\W4Communitynet\\": "Classes/"
        }
    },
    "support": {
        "issues": "https://w4services.atlassian.net/servicedesk/customer/portal/2"
    },
    "replace": {
        "typo3-ter/w4_communitynet": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "w4_communitynet"
        }
    }
}
